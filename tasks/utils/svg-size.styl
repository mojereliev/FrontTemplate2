.icon
  display inline-block
  height 1em
  width 1em
  vertical-align middle
  fill inherit
  fill currentColor
  stroke inherit
  transition all .2s ease

  use
    fill inherit
    fill currentColor

<% _.each(icons, function (icon) { %>
.<%= icon.className %>  {
  font-size <%= icon.height %>
  width (<%= icon.width %>/<%= icon.height %>)em
}
<% });%>